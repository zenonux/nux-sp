<script>
  $('.section-rank  .tabs .tab').click(function () {
    var index = $(this).index()
    $(this)
      .addClass('tab-active')
      .siblings()
      .removeClass('tab-active')
    $('.section-rank .tab-content')
      .eq(index)
      .show()
      .siblings()
      .hide()
  })
  $('.section-rank .tab')
    .eq(0)
    .trigger('click')
  $('.section-recommend  .tabs .tab').click(function () {
    var index = $(this).index()
    $(this)
      .addClass('tab-active')
      .siblings()
      .removeClass('tab-active')
    $('.section-recommend .tab-content')
      .eq(index)
      .show()
      .siblings()
      .hide()
  })
  $('.section-recommend .tab')
    .eq(0)
    .trigger('click')
  var mySwiper = new Swiper('.banner', {
